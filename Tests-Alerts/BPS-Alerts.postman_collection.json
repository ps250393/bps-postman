		{
			"name": "Get Alerts",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "3d5670c9-7aaa-489b-88b0-3ca40f27579c",
						"exec": [
							"//Load pxlib",
							"var pxlib = eval(pm.globals.get(\"pxlib\"));",
							"",
							"//Start testing using pxlib",
							"startPXTest(pm, false);",
							"",
							"//Validate response status code",
							"expectResponseStatusCodeToBe(200);",
							"",
							"//Validate response body",
							"",
							"let convertedData = xml2Json(pm.response.text());",
							"tests[\"Count: \"  + convertedData.Success.AlertPreferences.AlertPreference.length] = true;",
							"tests[\"Got Alerts successfully\"] = pm.expect(pm.response.text()).to.include('Success');",
							"tests[\"Verify id\"] = pm.expect(pm.response.text()).to.include('id');",
							"tests[\"Verify Alerts\"] = pm.expect(pm.response.text()).to.include('AlertPreferences');",
							"tests[\"Verify Alertpref\"] = pm.expect(pm.response.text()).to.include('AlertPreference');",
							"tests[\"verify metadata\"] = pm.expect(pm.response.text()).to.include('metaData');",
							"",
							"expectResponseBodyToHaveJsonUnorderedArray(\"Success.AlertPreferences.AlertPreference\", [",
							"        {pathToProperty: \"id._\", expectedValue: pm.environment.get(\"Alertid\")},",
							"        {pathToProperty: \"revision\"},",
							"        {pathToProperty: \"metaData.createdTime\", expectedValue: \"2016-09-20T07:37:15.804-07:00\"},",
							"        {pathToProperty: \"metaData.lastUpdatedTime\"},",
							"        {pathToProperty: \"name\", expectedValue: \"Payment Due\"},",
							"        {pathToProperty: \"description\", expectedValue: \"Notification - Your {payee nickname} e-bill payment is due\"},",
							"        {pathToProperty: \"status\", expectedValue: \"ON\"},",
							"        {pathToProperty: \"category\", expectedValue: \"EBILL\"}",
							"        ]);",
							"",
							"//pm.test(\"Check Payee Name\", function () {",
							"//    pm.expect(convertedData.Success.Payees.Payee.name).to.equal(\"ManagePayee\");",
							"//    pm.expect(convertedData.Success.Payees.Payee.status).to.equal(\"ACTIVE\");",
							"//});",
							"",
							"//End testing using pxlib",
							"endPXTest();",
							"",
							"var contentTypeHeaderExists = responseHeaders.hasOwnProperty(\"Content-Type\");",
							"tests[\"Has Content-Type\"] = contentTypeHeaderExists;",
							"",
							"",
							""
						],
						"type": "text/javascript"
					}
				}
			],
